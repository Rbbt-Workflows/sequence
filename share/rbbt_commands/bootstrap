require 'rbbt/workflow'

Workflow.require_workflow "Sequence"

mutations = {}

mutations_hg18, mutations_hg19 = MUTATIONS.split("@").collect{|l| l.split("\n").collect{|m| m.strip} }  

mutations["Hsa/may2009"] = mutations_hg18
mutations["Hsa/jun2011"] = mutations_hg19
mutations["Hsa/jan2013"] = mutations_hg19
mutations["Hsa"] = mutations_hg19

jobs = []
%w(Hsa Hsa/jun2011 Hsa/may2009 Hsa/jan2013).each do |organism|
    params = { :mutations => mutations[organism].dup, :organism => organism }

    job = Sequence.job(:mutated_isoforms_for_genomic_mutations, "BOOTSTRAP", params)
    job.clean
    jobs << job

    params = { :positions => mutations[organism].dup, :organism => organism }
    job = Sequence.job(:exon_junctions_at_genomic_positions, "BOOTSTRAP", params)
    job.clean
    jobs << job

    params = { :positions => mutations[organism].dup, :organism => organism }
    job = Sequence.job(:genes_at_genomic_positions, "BOOTSTRAP", params)
    job.clean
    jobs << job
end

jobs.shuffle! 

procs = ARGV.shift || "1"
require 'rbbt/util/semaphore'
RbbtSemaphore.fork_each_on_semaphore(jobs, procs.to_i) do |job|
  job.run(true)
end

